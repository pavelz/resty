-title "Editing event"
.event
  .title
    Update Event
  = error_messages_for :event

  - form_for(:event, :url => object_url, :html => { :method => :put }) do |f|
    = render :partial => "form", :locals => {:f => f}
    %p= submit_tag "Update"
  .edit><
    = link_to 'Show', object_path
    = link_to 'Back', objects_path
